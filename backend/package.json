{
  "name": "financial-analyst-backend",
  "version": "1.0.0",
  "description": "Advanced financial analysis platform backend with real-time market data, technical analysis, and ML predictions",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run lint && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "financial",
    "analysis",
    "trading",
    "stock-market",
    "technical-analysis",
    "machine-learning",
    "api"
  ],
  "author": "Financial Analyst Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "yahoo-finance2": "^2.8.1",
    "technicalindicators": "^3.1.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "prom-client": "^15.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abhishekjc19/-financial-analyst.git"
  },
  "bugs": {
    "url": "https://github.com/Abhishekjc19/-financial-analyst/issues"
  },
  "homepage": "https://abhishekjc19.github.io/-financial-analyst/"
} 